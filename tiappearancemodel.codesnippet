	<key>IDECodeSnippetContents</key>
	<string>extension &lt;#View#&gt; {

    public final class Layout: UIView.BaseLayout, ViewLayout {

        public static var defaultLayout: Layout {
            .init()
        }
    }

    public final class Appearance: UIView.BaseAppearance&lt;Layout&gt;, ViewAppearance {

        public static var defaultAppearance: Appearance {
            .init()
        }

        public override init(layout: &lt;#View#&gt;.Layout = .defaultLayout,
                             backgroundColor: UIColor? = nil,
                             roundedCorners: CACornerMask = [],
                             cornerRadius: CGFloat = .zero,
                             shadow: UIViewShadow? = nil) {

            super.init(layout: layout,
                       backgroundColor: backgroundColor,
                       roundedCorners: roundedCorners,
                       cornerRadius: cornerRadius,
                       shadow: shadow)
        }
    }
}</string>
